programa
{
	
	funcao inicio()
	{
		real temp
		
		escreva("Qual é a temperatura?\n")
		leia(temp)

		se(temp>=0)
		escreva("A temperatura é possitiva")

		senao
		escreva("A temperatura é negativa")
	}
}

programa
{
	
	funcao inicio()
	{
		inteiro n
		inteiro n2
		
		escreva("Digite o primeiro número: ")
		leia(n)
		escreva("Digite o segundo número: ")
		leia(n2)

		se (n > n2){
		escreva(n," é maior que ", n2)
		
		}senao{
		escreva(n2," é maior que ", n)
		}
	}
}
